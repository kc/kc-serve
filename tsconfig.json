{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitThis": true,
        "importHelpers": true,
        "jsx": "react",
        "jsxFactory": "elements.createElement"
    }
}